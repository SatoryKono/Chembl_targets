  "comments": [
    {
      "texts": [
        {
          "evidences": [
            {
              "evidenceCode": "ECO:0000250"
            },
            {
              "evidenceCode": "ECO:0000269",
              "source": "PubMed",
              "id": "11274989"
            },
            {
              "evidenceCode": "ECO:0000269",
              "source": "PubMed",
