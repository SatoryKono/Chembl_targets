      "note": {
        "texts": [
          {
            "evidences": [
              {
                "evidenceCode": "ECO:0000250",
                "source": "UniProtKB",
                "id": "O76074"
              }
            ],
            "value": "Binds 1 Mg(2+) ions per subunit. Binds 2 divalent metal cations per subunit: site 1 preferentially binds zinc, while site 2 has a preference for magnesium. Binds magnesium less tightly than zinc."
          }
        ]
      }
    },
    {
      "texts": [
        {
          "value": "Inhibited by zaprinast"
        }
      ],
